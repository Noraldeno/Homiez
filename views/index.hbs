<!--Page Header/Jumbotron-->
<div class="container-fluid">
	<div class="jumbotron">
		<h1>{{title}}</h1>
			<form action={{search}} method="post">
				<input type="text" name="searchBar" placehoder="Search Listings">
				<input type="submit" value="Go" id="submitThis">	
			</form>
	</div>
</div>

{{!-- Example call to an image --}}
