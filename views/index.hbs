<!--Search Results-->
<div class="container-fluid">
	<div class="row">
		{{#each listItems}}
			<div class="col-sm-4 house">
				<p>{{this.address}}</p>
				<p>{{this.zip}}</p>
				<p>{{this.city}}</p>
			</div>
		{{/each}}
	</div>
</div>


